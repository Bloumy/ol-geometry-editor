  "repository": {
    "type": "git",
    "url": "http://gitlab.dockerforge.ign.fr/sai/ol-geometry-editor.git"
  },
  "author": "Mickaël Borne <mickael.borne@ign.fr>, Alexis Hordé <alexis.horde@ign.fr>",
  "license": "BSD-2-Clause",
  "dependencies": {
    "jquery": "^3.3.1",
    "openlayers": "^4.6.4",
    "turf-bbox-polygon": "^1.0.2",
    "turf-extent": "^1.0.4"
  },
  "devDependencies": {
    "grunt-cli": "^1.2.0",
    "bootstrap": "^3.3.6",
    "browserify": "~13.0.0",
    "browserify-shim": "^3.8.12",
    "deamdify": "~0.2.0",
    "debowerify": "^1.5.0",
    "decomponentify": "~0.1.0",
    "deglobalify": "~0.2.0",
    "expect": "^1.15.2",
    "grunt": "^1.0.3",
    "grunt-browserify": "^5.3.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "~0.8.2",
    "grunt-contrib-jshint": "^0.12.0",
    "grunt-contrib-uglify": "~0.11.1",
    "grunt-contrib-watch": "^1.1.0",
    "mocha": "^5.2.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "debowerify",
      "decomponentify",
      "deamdify",
