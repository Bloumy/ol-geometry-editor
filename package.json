  "dependencies": {
    "grunt-cli": "^0.1.13",
    "jquery": "^2.2.0",
    "openlayers" : "^4.6.4",
    "turf-bbox-polygon": "^1.0.2",
    "turf-extent": "^1.0.4"
  },
  "devDependencies": {
    "bootstrap": "^3.3.6",
    "browserify": "~13.0.0",
    "browserify-shim": "^3.8.12",
    "deamdify": "~0.2.0",
    "debowerify": "~1.3.1",
